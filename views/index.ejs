<div class="container ">
  <div class="courses">
      <div class="card">
          <div class="card-header">
          <strong class="display-4"> All Students</strong>
          </div>
          <div class="card-body">
          
          <% if (messages.success) { %>
                    <p style="color:green"><%- messages.success %></p>
                  <% } %>	
      <div class="row">
          <div class="col">
              <div class="table-responsive ">
                  <table class="table table-striped table-bordered" id="mytable" name="mytable">
                  
                    <tr style='text-align:left; background-color:#CCC'>
                      <th>Name</th>
                      <th>Course</th>
                      <th>Email</th>
                      <th>Action</th>
                    </tr>
                    
                    <!--
                      Using FOREACH LOOP for the users array
                      
                      myArray.forEach(function(el, index) {
                        // el - current element, i - index
                      });
                    -->
                  
                    <% if (data1) { %>
                    <% data1.forEach(function(user1){ %>
                      <tr>
                        <td><%= user1.name %></td>
                        <td><%= user1.course %></td>
                        <td><%= user1.email %></td>
                        <td>
                          <div style="float:left">
                            <a href='/edit/<%= user1.id %>' class="btn btn-sm btn-success">Edit</a> &nbsp;							
                            <form method="post" action="/<%= user1.id %>" style="float:right">
                              <input type="submit" class="btn btn-sm btn-danger" name="delete" value='Delete' onClick="return confirm('Are you sure you want to delete?')" />
                              <input type="hidden" name="_method" value="DELETE" />
                            </form>
                          </div>
                        </td>
                      </tr>
                    <% }) %>
                    <% } %>
                  
                  </table>
                </div>
                  
  </div>
  
      
          
