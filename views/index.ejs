            <% if (messages.error) { %>
              <p style="color:red"><%- messages.error %></p>
            <% } %>
            <form action="/add" method="post"  name="form1">

          
          <div class="row">
            <div class="col-xs-12 col-sm-6 col-md-6 col-lg-6">
                <div class="form-group">
                    <input type="text" name="name" value="<%= name %>" class="form-control input-lg" placeholder="First Name" tabindex="1" required>
                </div>
            </div>
          <div class="col-xs-12 col-sm-6 col-md-6 col-lg-6">
              <div class="form-group">
                  <input type="text"  class="form-control input-lg" placeholder="Last Name" tabindex="2">
