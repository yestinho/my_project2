<% if (messages.error) { %>
	<p style="color:red"><%- messages.error %></p>
<% } %>

<% if (messages.success) { %>
	<p style="color:green"><%- messages.success %></p>
<% } %>

<div class="container h-100" style="margin-top:12%;">
    <div class="row h-100">
        <div class="col-md-4 my-auto">
            <div class="header-content mx-auto">
                <h1 class="text-white">E-Learning is learning utilizing electronic technologies to access educational curriculum outside of a traditional classroom. </h1>
            </div>
        </div>
        <div class="col-lg-8">
            <h2 class="text-white">Please Register for upcoming Courses</h2>
            <% if (messages.error) { %>
              <p style="color:red"><%- messages.error %></p>
            <% } %>
            <form action="/edit/<%= id %>" method="post"  name="form1">

          
          <div class="row">
            <div class="col-xs-12 col-sm-6 col-md-6 col-lg-6">
                <div class="form-group">
                    <input type="text" name="name" value="<%= name %>" class="form-control input-lg" placeholder="First Name" tabindex="1" required>
                </div>
            </div>
          <div class="col-xs-12 col-sm-6 col-md-6 col-lg-6">
              <div class="form-group">
                  <input type="text"  class="form-control input-lg" placeholder="Last Name" tabindex="2">
              </div>
          </div>
          <div class="col-xs-12 col-sm-6 col-md-6 col-lg-6">
            <div class="form-group">
                <input type="text" name="email" value="<%= email %>" class="form-control input-lg" placeholder="Email " tabindex="4" required>
                
            </div>
        </div>
      <div class="col-xs-12 col-sm-6 col-md-6 col-lg-6">
          <div class="form-group">
              <input type="text" class="form-control input-lg" placeholder="Phone" tabindex="3" >
          </div>
      </div>
      <div class="col-xs-12 col-sm-6 col-md-6 col-lg-6">
        <div class="form-group">
          <select name="course" value="<%= course %>" class="form-control" id ="select">
            <option selected>Choose...</option>
            <option value="One">One</option>
            <option value="Two">Two</option>
            <option value="Three">Three</option>
          </select>
        </div>
    </div>
  <div class="col-xs-12 col-sm-6 col-md-6 col-lg-6">
      <div class="form-group">
          <input type="text" class="form-control input-lg" placeholder="Your Education Level" tabindex="5">
      </div>
  </div>
  </div>
  <div class="row">
      <div class="col-md-12"><input type="submit" name="Submit" value="Update
							Now" class="btn btn-info btn-block btn-lg">
							<input type="hidden" name="_method" value="PUT" />

						</div>
							
  </div>
        </form>
      </div>
          
              
      
    </div>

</div>

